# PrintPilot Environment Variables Template
# Copy this to .env.local and fill in your values

# Environment
NODE_ENV="development"

# Database - Supabase PostgreSQL
DATABASE_URL="postgresql://postgres.[PROJECT_ID]:[PASSWORD]@aws-0-eu-central-1.pooler.supabase.com:5432/postgres"

# NextAuth.js
NEXTAUTH_SECRET="[GENERATE_NEW_SECRET]"
NEXTAUTH_URL="http://localhost:3000"

# Supabase
NEXT_PUBLIC_SUPABASE_URL="https://[PROJECT_ID].supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="[YOUR_ANON_KEY]"
SUPABASE_SERVICE_ROLE_KEY="[YOUR_SERVICE_ROLE_KEY]"

# WooCommerce Integration
WOOCOMMERCE_API_URL="https://yourshop.com/wp-json/wc/v3"
WOOCOMMERCE_CONSUMER_KEY="ck_your_consumer_key"
WOOCOMMERCE_CONSUMER_SECRET="cs_your_consumer_secret"

# Adobe Stock API
ADOBE_STOCK_API_KEY="your_adobe_stock_api_key"

# Shipping Providers
